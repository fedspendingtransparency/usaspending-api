      "method": "GIN",
      "columns": [{"name": "business_categories"}]
    }, {
      "name": "keyword_ts_vector",
      "method": "GIN",
      "columns": [{"name": "keyword_ts_vector"}]
    }, {
      "name": "award_ts_vector",
      "method": "GIN",
      "columns": [{"name": "award_ts_vector"}]
    }, {
