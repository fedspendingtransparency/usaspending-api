{
  "final_name": "summary_view_psc_codes",
  "matview_sql": [
    "SELECT",
    "  \"transaction_normalized\".\"action_date\",",
    "  \"transaction_normalized\".\"fiscal_year\",",
    "  \"transaction_normalized\".\"type\",",
    "  \"transaction_fpds\".\"pulled_from\",",
    "  \"transaction_fpds\".\"product_or_service_code\",",
    "  SUM(COALESCE(\"transaction_normalized\".\"federal_action_obligation\", 0)) AS \"federal_action_obligation\",",
    "  0::NUMERIC AS \"original_loan_subsidy_cost\",",
    "  COUNT(*) counts",
    "FROM",
    "  \"transaction_normalized\"",
    "INNER JOIN",
    "  \"transaction_fpds\" ON (\"transaction_normalized\".\"id\" = \"transaction_fpds\".\"transaction_id\")",
    "WHERE",
    "  \"transaction_normalized\".action_date >= '2007-10-01'",
    "GROUP BY",
    "  \"transaction_normalized\".\"action_date\",",
    "  \"transaction_normalized\".\"fiscal_year\",",
    "  \"transaction_normalized\".\"type\",",
    "  \"transaction_fpds\".\"pulled_from\",",
    "  \"transaction_fpds\".\"product_or_service_code\""
  ],
  "indexes": [
    {
      "name": "action_date",
      "columns": [{"name": "\"action_date\"", "order": "DESC NULLS LAST"}]
    }, {
      "name": "type",
      "columns": [
        {"name": "\"action_date\"", "order": "DESC NULLS LAST"},
        {"name": "\"type\""}
      ]
    }, {
      "name": "tuned_type_and_idv",
      "where": "\"type\" IS NULL AND \"pulled_from\" IS NOT NULL",
      "columns": [{"name": "\"type\""}, {"name": "\"pulled_from\""}]
     }
  ]
}
